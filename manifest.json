{
	"manifest_version":2,
	"name":"Titre WIKI",
	"description":"Affichez le Titre de la page web actif dans un fichier .",
	"version":"1.0",
	"permissions": [ "tabs" ],
	"content_scripts":[
	{
		"matches":["<all_urls>"],
		"js":["script.js"]
		}
	],
	"browser_action":{
		"default_icon":"icon.png",
		"default_popup":"popup.html"
	}
}
